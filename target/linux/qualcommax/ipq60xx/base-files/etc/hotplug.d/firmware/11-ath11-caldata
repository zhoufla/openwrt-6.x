#!/bin/sh

[ -e /lib/firmware/$FIRMWARE ] && exit 0

. /lib/functions/caldata.sh

board=$(board_name)

case "$FIRMWARE" in
"ath11k/IPQ6018/hw1.0/cal-ahb-c000000.wifi.bin")
	case "$board" in
	8devices,mango-dvk)
		caldata_extract "0:ART" 0x1000 0x20000
		;;
	glinet,gl-ax1800|\
	glinet,gl-axt1800|\
	netgear,wax214)
		caldata_extract "0:art" 0x1000 0x10000
		;;
	cmiot,ax18|\
	zn,m2|\
	qihoo,v6|\
	redmi,ax5|\
	xiaomi,ax1800)
		caldata_extract "0:art" 0x1000 0x10000
		;;
	redmi,ax5-jdcloud|\
	jdc,ax1800-pro)
		caldata_extract_mmc "0:ART" 0x1000 0x10000
		;;
	yuncore,fap650)
		caldata_extract "0:art" 0x1000 0x20000
		;;
	esac
	;;
*)
	exit 1
	;;
esac
